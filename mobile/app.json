{
  "expo": {
    "name": "SwimTrainApp",
    "slug": "swim-train-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "sdkVersion": "53.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swimteam.swimtrainapp"
    },
    "android": {
      "package": "com.swimteam.swimtrainapp",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "swim-train-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}
